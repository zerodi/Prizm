{
  "$schema": "http://json-schema.org/schema",
  "version": 2,
  "title": "IconsToLazy executor",
  "description": "",
  "type": "object",
  "properties": {
    "pathToFolder": {
      "type": "string",
      "alias": "i",
      "description": "Path to folder with svg"
    },
    "prefix": {
      "type": "string",
      "alias": "i",
      "description": "File prefix for ignore"
    },
    "exportName": {
      "type": "string",
      "alias": "i",
      "description": "Name of exported constant"
    },
    "addExportToSingleFile": {
      "type": "boolean",
      "description": "Add all file exports to single file"
    },
    "pathToOutputFile": {
      "type": "string",
      "alias": "o",
      "description": "Path to output with lazy imports"
    }
  },
  "required": ["pathToFolder", "pathToOutputFile"]
}
